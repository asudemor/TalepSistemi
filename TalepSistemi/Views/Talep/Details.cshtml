@model Talep

@{
    ViewData["Title"] = "Detay Sayfası";
}
<h1>@ViewData["Title"]</h1>

<div>
    <p>Talep ID: @Model.TalepID</p>
    <p>Talep Gonderen ID: @Model.TalepGonderenID</p>
    <p>Talep Departman: @Model.TalepDepartman</p>
    <p>Talep Konu: @Model.TalepKonu</p>
    <p>Talep Açıklama: @Model.TalepAciklama</p>
    <p>Talep Tarih: @Model.TalepTarih</p>
    <p>Talep Durum: @Model.TalepDurum</p>
</div>